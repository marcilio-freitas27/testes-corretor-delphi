unit Corretor;

interface

type
  // Criação da classe
  TCorretor = class
  // Criando o bloco de funções como público
  nome: String;
  data: String;
  salário: Single;
  lucroMensal: Integer;
  public
    function CalcularComissao(): Single;
  end;

implementation

{ TCorretor }

function TCorretor.Comissao: Single;
begin
  if True then
  begin
    Result := 1;
  end
  else
    Result := 1;
  end;
end;

end.
